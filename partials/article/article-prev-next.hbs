<div class="prev-next-item u-relative {{#if class}}{{class}}{{/if}}">
    <a class="prev-next-link" href="{{url}}">
        {{#if feature_image}}
        <img class="prev-next-img u-image u-absolute0 blur-up lazyload"
            src="{{img_url feature_image size="m"}}"
            srcset="{{img_url feature_image size="xxs"}}"
            data-srcset="{{img_url feature_image size="s"}} 300w,
                    {{img_url feature_image size="m"}} 600w,
                    {{img_url feature_image size="l"}} 1000w"
            data-sizes="(max-width: 1000px) 400px, 700px"
            alt="{{title}}"
        />
        {{/if}}
    </a>

    <a href="{{url}}" class="prev-next-body">
        <span class="u-meta">{{mytitle}}</span>
        <span class="prev-next-title">{{title}}</span>
    </a>
</div>